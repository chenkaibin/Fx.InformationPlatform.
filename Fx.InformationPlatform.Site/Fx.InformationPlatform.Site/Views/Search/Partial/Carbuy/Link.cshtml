@model Fx.InformationPlatform.Site.ViewModel.CarBuySearchModel
@if (Model.CurrentIndex == 1)
{
    @Html.ActionLink("上一页", Model.Action, new
        {
            page = 1,
            key = Model.Key,
            area = Model.AreaId,
            city = Model.CityId,
            clc = Model.Clc
        }, new { @class = "pre-btn",rel="nofollow" })
}
else
{
    @Html.ActionLink("上一页", Model.Action, new
        {
            page = Model.CurrentIndex - 1,
            key = Model.Key,
            area = Model.AreaId,
            city = Model.CityId,
            clc = Model.Clc
        }, new { @class = "pre-btn",rel="nofollow" })
}
@for (int i = Model.StartIndex; i <= Model.EndIndex; i++)
{
    if (i == Model.CurrentIndex)
    {
    <a href="#" class="currents-pages" rel="nofollow">@Model.CurrentIndex</a>
    }
    else
    {
    @Html.ActionLink(i.ToString(), Model.Action, new
        {
            page = i,
            key = Model.Key,
            area = Model.AreaId,
            city = Model.CityId,
            clc = Model.Clc
        }, new { rel="nofollow" })
    }
}
@Html.ActionLink("下一页", Model.Action, new
    {
        page = Model.CurrentIndex + 1,
        key = Model.Key,
        area = Model.AreaId,
        city = Model.CityId,
        clc = Model.Clc
    }, new { @class = "next-btn",rel="nofollow" })
